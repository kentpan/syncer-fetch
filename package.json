{
  "name": "syncer-fetch",
  "version": "1.0.0",
  "description": "一个适合并发上传下载二进制内容的fetch封装库，支持代理、并发控制、重试机制等功能。",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "fetch",
    "http",
    "request",
    "proxy",
    "concurrency",
    "retry",
    "promise",
    "binary",
    "upload",
    "download",
    "typescript"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/syncer-fetch.git"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0"
  }
}
